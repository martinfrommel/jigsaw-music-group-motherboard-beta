datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

// Defined User Model

model User {
  id             Int @id @default(autoincrement())
  firstName String
  lastName String
  email          String  @unique
  hashedPassword      String  
  salt                String    
  resetToken          String?   
  resetTokenExpiresAt DateTime?
}




